{
  "name": "post-it-app",
  "version": "1.0.0",
  "description": "PostIt is an app that allows users send broadcast messages to other users in groups",
  "main": "index.js",
  "scripts": {
    "dev": "babel-watch server/server.js",
    "start": "npm run build && babel-node server/server.js",
    "build": "webpack",
    "test": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OsayamenOsaretin/post-it-app.git"
  },
  "author": "o.osayamen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OsayamenOsaretin/post-it-app/issues"
  },
  "homepage": "https://github.com/OsayamenOsaretin/post-it-app#readme",
  "jest": {
    "collectCoverage": true,
    "moduleNameMapper": {
      "^SignInAction$": "<rootDir>/client/src/data/postItActions/userSignInAction.js",
      "^RegisterAction$": "<rootDir>/client/src/data/postItActions/registerUserAction.js",
      "^GroupAction$": "<rootDir>/client/src/data/postItActions/groupActions.js",
      "^UserListAction$": "<rootDir>/client/src/data/postItActions/addUserGroup",
      "^SendMessageAction$": "<rootDir>/client/src/data/postItActions/sendMessageAction",
      "^GetAllUsersAction$": "<rootDir>/client/src/data/postItActions/getAllUsersAction",
      "^PostItDispatcher$": "<rootDir>/client/src/data/PostItDispatcher",
      "^GetMessagesAction$": "<rootDir>/client/src/data/postItActions/getMessagesAction",
      "^GroupActions$": "<rootDir>/client/src/data/postItActions/groupActions",
      "^AddUserGroup$": "<rootDir>/client/src/data/postItActions/addUserGroup"
    }
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-watch": "^2.0.6",
    "body-parser": "^1.17.2",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "file-loader": "^0.11.2",
    "firebase": "^4.0.0",
    "flux": "^3.1.2",
    "html-webpack-plugin": "^2.28.0",
    "img-loader": "^2.0.0",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.1",
    "nexmo": "^2.0.2",
    "nodemailer": "^4.0.1",
    "nodemailer-direct-transport": "^3.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^1.3.1",
    "react-icons": "^2.2.5",
    "react-immutable-proptypes": "^2.1.0",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-tools": "^0.13.3",
    "react-transform": "0.0.3",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "style-loader": "^0.18.2",
    "superagent": "^3.5.2",
    "url-loader": "^0.5.8",
    "webpack": "^3.3.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "chai": "^4.0.0",
    "coveralls": "^2.13.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx": "0.0.2",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.4",
    "mockery": "^2.1.0",
    "react-test-renderer": "^15.6.1",
    "sinon": "^2.3.8",
    "superagent-mocker": "^0.5.2",
    "supertest": "^3.0.0",
    "webpack-dev-server": "^2.4.5"
  }
}
